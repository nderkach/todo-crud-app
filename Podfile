platform :ios, '9.0'

target 'CMX' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CMX

  # Networking 
  pod 'JWTDecode', '~> 2.1'
  pod 'Siesta', '~> 1.0'
  pod 'Siesta/Alamofire', '~> 1.0'
  
  # Misc
  pod 'SnapKit', '~> 4.0.0'
  pod 'OpenSans'
  pod 'KeychainAccess'

  target 'CMXTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CMXUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
